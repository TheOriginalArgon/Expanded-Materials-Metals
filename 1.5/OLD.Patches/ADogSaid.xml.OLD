<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<!-- EPOE Buildings -->
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>A Dog Said... Animal Prosthetics</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/ThingDef [defName = "ProstheticTailAnimal"]/costList</xpath>
					<value>
						<costList>
							<Steel>10</Steel>
							<VMEu_Iron>15</VMEu_Iron>
						</costList>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/ThingDef [defName = "SimpleProstheticLegAnimal"]/costList</xpath>
					<value>
						<costList>
							<Steel>10</Steel>
							<VMEu_Iron>10</VMEu_Iron>
							<ComponentIndustrial>2</ComponentIndustrial>
						</costList>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/ThingDef [defName = "SimpleProstheticArmAnimal"]/costList</xpath>
					<value>
						<costList>
							<Steel>10</Steel>
							<VMEu_Iron>10</VMEu_Iron>
							<ComponentIndustrial>2</ComponentIndustrial>
						</costList>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/ThingDef [defName = "SpineAnimalSimple"]/costList</xpath>
					<value>
						<costList>
							<Steel>10</Steel>
							<VMEu_Iron>5</VMEu_Iron>
							<ComponentIndustrial>2</ComponentIndustrial>
						</costList>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/ThingDef [defName = "BionicLegAnimal"]/costList</xpath>
					<value>
						<costList>
							<Plasteel>10</Plasteel>
							<VMEu_StainlessSteel>10</VMEu_StainlessSteel>
							<ComponentSpacer>2</ComponentSpacer>
						</costList>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/ThingDef [defName = "BionicArmAnimal"]/costList</xpath>
					<value>
						<costList>
							<Plasteel>10</Plasteel>
							<VMEu_StainlessSteel>10</VMEu_StainlessSteel>
							<ComponentSpacer>2</ComponentSpacer>
						</costList>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/ThingDef [defName = "SpineAnimalBionic"]/costList</xpath>
					<value>
						<costList>
							<Plasteel>10</Plasteel>
							<VMEu_Titanium>5</VMEu_Titanium>
							<VMEu_StainlessSteel>10</VMEu_StainlessSteel>
							<ComponentSpacer>2</ComponentSpacer>
						</costList>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/ThingDef [defName = "AnimalBrainStimulator"]/costList</xpath>
					<value>
						<costList>
							<Plasteel>50</Plasteel>
							<VMEu_Chromium>8</VMEu_Chromium>
							<ComponentSpacer>2</ComponentSpacer>
						</costList>
					</value>
				</li>
			</operations>
		</match>
	</Operation>

</Patch>