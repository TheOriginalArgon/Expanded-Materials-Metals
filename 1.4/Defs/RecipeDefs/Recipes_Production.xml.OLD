<Defs>

  <!-- Make Charcoal -->
  <RecipeDef>
    <defName>VMEu_MakeCharcoal</defName>
    <label>make charcoal</label>
    <description>Burn wood in order to obtain charcoal, it is not so good as mineral coal, but still a good replacement.</description>
    <jobString>Making Charcoal.</jobString>
    <effectWorking>Smelt</effectWorking>
    <soundWorking>Recipe_Smelt</soundWorking>
    <workAmount>700</workAmount>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <recipeUsers>
      <li>ElectricSmelter</li>
      <li>Campfire</li>
      <li>VMEu_MetalSmelter</li>
      <li>VMEu_ElectricMetalSmelter</li>
      <li>VMEu_AdvancedSmelter</li>
    </recipeUsers>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>WoodLog</li>
          </thingDefs>
        </filter>
        <count>30</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>WoodLog</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <VMEu_Charcoal>25</VMEu_Charcoal>
    </products>
  </RecipeDef>

