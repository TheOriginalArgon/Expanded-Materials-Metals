    <!-- =============================== CHARCOAL RECIPES =============================== -->

    <RecipeDef ParentName="AlloyBase">
        <defName>EM_Make_Charcoal</defName>
        <label>make charcoal</label>
        <description>Incinerate wood in a closed chamber. Lack of oxygen prevents the wood from completely burning, thus making charcoal.</description>
        <jobString>Making charcoal.</jobString>
        <workAmount>1900</workAmount> <!-- Placeholder -->
        <effectWorking>Cremate</effectWorking>
        <soundWorking>Recipe_Cremate</soundWorking>
        <workSpeedStat>SmeltingSpeed</workSpeedStat>
        <recipeUsers>
            <li>EM_FueledAlloyFurnace</li>
            <li>EM_ElectricAlloyFurnace</li>
            <li>FueledSmithy</li>
            <li>ElectricSmithy</li>
        </recipeUsers>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>WoodLog</li>
                    </thingDefs>
                </filter>
                <count>35</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>WoodLog</li>
            </thingDefs>
        </fixedIngredientFilter>
        <products>
            <EM_Charcoal>35</EM_Charcoal>
        </products>
        <researchPrerequisite>Smithing</researchPrerequisite>
    </RecipeDef>

