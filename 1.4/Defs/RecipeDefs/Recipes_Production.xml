  <!-- Make Rock Powder -->
  <RecipeDef>
    <defName>VMEu_MakeRockPowder</defName>
    <label>make primitive cement</label>
    <description>Ground stones to make primitive cement, an useful material resistant to fire used commonly to build cheap walls and floors.</description>
    <jobString>Making primitive cement.</jobString>
    <effectWorking>CutStone</effectWorking>
    <soundWorking>Recipe_MakeStoneBlocks</soundWorking>
    <workAmount>1000</workAmount>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <recipeUsers>
      <li>TableStonecutter</li>
    </recipeUsers>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>StoneChunks</li>
          </categories>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <categories>
        <li>StoneChunks</li>
      </categories>
    </fixedIngredientFilter>
    <products>
      <VMEu_RockPowder>50</VMEu_RockPowder>
    </products>
    <researchPrerequisite>Stonecutting</researchPrerequisite>
  </RecipeDef>

