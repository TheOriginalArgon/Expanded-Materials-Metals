<Defs>

  <!-- ================================ COAL TYPES ================================ -->

  <!-- Coal -->
  <ThingDef ParentName="ResourceBase">
    <defName>EM_Coal</defName>
    <label>coal</label>
    <description>A combustible, black sedimentary rock formed by dead plant matter turned into peat and converted into coal by the heat and pressure of deep burial over millions of years. Coal is mostly carbon with variable amounts of other elements. It is used as a common fuel and also as a source of carbon, which is required in some important alloys like steel.</description>
    <graphicData>
      <texPath>Things/Item/Resource/EM_Coal</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <soundInteract>ChunkRock_Drop</soundInteract>
    <soundDrop>ChunkRock_Drop</soundDrop>
    <useHitPoints>true</useHitPoints>
    <healthAffectsPrice>false</healthAffectsPrice>
    <statBases>
      <ArgonicCore_FuelPower>2.0</ArgonicCore_FuelPower>
      <MaxHitPoints>120</MaxHitPoints>
      <Flammability>0.8</Flammability>
      <DeteriorationRate>0.4</DeteriorationRate>
      <MarketValue>1</MarketValue>
      <Mass>0.35</Mass>
    </statBases>
    <thingCategories>
      <li>ResourcesRaw</li>
    </thingCategories>
    <deepCommonality>4</deepCommonality>
    <deepCountPerPortion>40</deepCountPerPortion>
    <deepLumpSizeRange>25~50</deepLumpSizeRange>
    <burnableByRecipe>false</burnableByRecipe>
    <smeltable>false</smeltable>
    <terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
  </ThingDef>

  <!-- Charcoal -->
  <ThingDef ParentName="ResourceBase">
    <defName>EM_Charcoal</defName>
    <label>charcoal</label>
    <description>A lightweight, black carbon residue produced by strongly heating wood to remove all water and volatile constituents. Charcoal is not as much of an efficient carbon source as mineral coal is, but it is an effective substitute.</description>
    <graphicData>
      <texPath>Things/Item/Resource/EM_Charcoal</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <soundInteract>ChunkRock_Drop</soundInteract>
    <soundDrop>ChunkRock_Drop</soundDrop>
    <useHitPoints>true</useHitPoints>
    <healthAffectsPrice>false</healthAffectsPrice>
    <statBases>
      <ArgonicCore_FuelPower>1.5</ArgonicCore_FuelPower>
      <MaxHitPoints>120</MaxHitPoints>
      <Flammability>0.8</Flammability>
      <DeteriorationRate>0.5</DeteriorationRate>
      <MarketValue>0.9</MarketValue>
      <Mass>0.35</Mass>
    </statBases>
    <thingCategories>
      <li>ResourcesRaw</li>
    </thingCategories>
    <terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
    <modExtensions>
      <li Class="ExpandedMaterials.ThingDefExtension">
        <fuelPower>1.7</fuelPower>
      </li>
    </modExtensions>
  </ThingDef>

