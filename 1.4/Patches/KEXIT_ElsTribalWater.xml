<?xml version="1.0" encoding="utf-8" ?>
<Patch>

<Operation Class="PatchOperationFindMod">
    <mods>
        <li>El's Tribal Water</li>
    </mods>
	<match Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/ThingDef[defName = "AdvancedPassiveCooler"]/costList</xpath>
				<value>
					<costList>
					  <WoodLog>60</WoodLog>
			          <VMEu_Iron>30</VMEu_Iron>
					</costList>
				</value>
			</li>
		</operations>
	</match>
</Operation>

</Patch>